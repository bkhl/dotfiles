! urxvt
URxvt.font: xft:monospace:antialias=true:hinting=false:pixelsize=12
URxvt.boldFont: xft:Monospace:bold:antialias=true:hinting=false:pixelsize=12
URxvt.scrollBar: false
URXvt.saveLines: 262144

! rofi
rofi.font: Sans 10
rofi.color-enabled: true
rofi.color-window: #3C3B37, #3C3B37, #285577
rofi.color-normal: #3C3B37, white, #3C3B37, #285577, white
rofi.color-active: #3C3B37, white, #3C3B37, #285577, white
rofi.color-urgent: #3C3B37, white, #3C3B37, #285577, white
