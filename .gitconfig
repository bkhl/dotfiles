	name = Björn Lindström
[include]
    path = .gitconfig.local
[push]
	autoSetupRemote = true
[alias]
	st = status
[fetch]
	parallel = 10
[pull]
	ff = only
[init]
	defaultBranch = main
[diff "elisp"]
    xfuncname="^\\((((def\\S+)|use-package)\\s+\\S+)"
[diff "org"]
    xfuncname="^\\*+ +(.*)$"
