[user]
	email = bkhl@elektrubadur.se
	name = Björn Lindström
[includeIf "gitdir:~/Work/"]
    path = .gitconfig.d/work
[push]
	default = current
[alias]
	st = status
[trim]
	detach = false
[fetch]
	parallel = 10
[pull]
	ff = only
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
